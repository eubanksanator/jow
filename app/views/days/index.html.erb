<p id="notice"><%= notice %></p>

<h1>Listing Days for week <%= @week.week_number %></h1>
<% @days.each do |day| %>
<ul>
  <li><%= day.name %></li>
  <% day.workouts.each do |workout| %>
  <ul>
    <li><%= workout.name %></li>
    <li><%= workout.sets %></li>
    <li><%= workout.reps %></li>
  </ul>
  <% end %>
</ul>
<% end %>

<%= link_to 'New Day', new_week_day_path %>
